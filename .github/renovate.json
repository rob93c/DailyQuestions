{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "labels": [
    "dependencies"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "automerge": true,
      "minimumReleaseAge": "1 week",
      "matchPackageNames": [
        "*"
      ]
    }
  ],
  "extends": [
    "config:recommended"
  ]
}
